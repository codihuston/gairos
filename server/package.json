{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "PRODUCTION": "-----------------------------------------------------------",
    "start": "npm run prod",
    "prod": "cross-env NODE_ENV=production npm-run-all clean build server:prod",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist --copy-files",
    "server:prod": "node ./dist/bin/www",
    "db:init": "sequelize-cli db:create --env production",
    "db:seed:production": "sequelize-cli db:seed:all --env production",
    "DEVELOPMENT": "----------------------------------------------------------",
    "lint": "eslint . --config .eslintrc-development.json",
    "dev": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm-run-all lint server",
    "server": "babel-node ./src/bin/www",
    "dev:debug": "npm run server:dev:debug",
    "server:dev:debug": "cross-env NODE_ENV=development DEBUG=server:* npm-run-all lint server",
    "watch": "nodemon --config nodemon.json",
    "watch:debug": "nodemon --exec \"npm run server:dev:debug\"",
    "db:seed:development": "sequelize-cli db:seed:all --env development",
    "TESTING    ": "----------------------------------------------------------",
    "lint:test": "eslint ./src/api/**/**/*.spec.js --config .eslintrc-test.json",
    "test": "npm-run-all lint:test test:unit test:int test:e2e",
    "test:unit": "cross-env NODE_ENV=test DEBUG=server:*,test:* jest .\\**\\unit.spec.js --config jest.config.json",
    "test:int": "cross-env NODE_ENV=test DEBUG=server:*,test:* jest .\\**\\int.spec.js --config jest.config.json",
    "test:e2e": "cross-env NODE_ENV=test DEBUG=server:*,test:* jest .\\**\\e2e.spec.js --config jest.config.json",
    "watch:test": "nodemon --config nodemon-test.json",
    "db:seed:test": "sequelize-cli db:seed:all --env test",
    "CI-CD      ": "----------------------------------------------------------",
    "ci": "cross-env NODE_ENV=ci test",
    "db:seed:ci": "sequelize-cli db:seed:all --env ci"
  },
  "dependencies": {
    "apollo-datasource": "^0.6.4",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.9.15",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "glob": "^7.1.6",
    "googleapis": "39",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.3.3",
    "graphql-tag": "^2.10.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.17.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "sequelize": "^5.21.3",
    "sinon": "^8.0.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "apollo-server-testing": "^2.9.16",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
